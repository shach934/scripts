FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     buoyantPimpleNFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

//endTime         70;
endTime         1.1;

deltaT          1e-2;

writeControl    runTime;

writeInterval   0.01;

purgeWrite      0;

writeFormat     ascii;

writePrecision  16;

writeCompression off;

timeFormat      general;

timePrecision   9;

runTimeModifiable yes;

maxCo           0.6;

adjustTimeStep  no;

